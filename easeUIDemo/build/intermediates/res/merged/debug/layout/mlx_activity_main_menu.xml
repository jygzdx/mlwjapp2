            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:drawableRight="@drawable/mlx_roght_goto"
            android:paddingLeft="20dip"
            android:paddingRight="20dip"
            android:text="@string/set_about"
            android:textColor="@android:color/black"
            android:textSize="@dimen/size_set"
            android:typeface="serif" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:textColor="@android:color/black" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:layout_alignParentBottom="true"
            android:background="#E6E6E6" />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#E6E6E6"/>
    <Button
        android:id="@+id/btn_set_out"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/em_btn_style_alert_dialog_special"
        android:paddingBottom="7dp"
        android:paddingTop="7dp"
        android:onClick="click"
        android:text="@string/set_out"
        android:textColor="@android:color/white"
        android:textSize="18sp"
         />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#E6E6E6"/>
