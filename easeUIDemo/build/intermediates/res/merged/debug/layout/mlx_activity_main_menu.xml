    <RelativeLayout
        android:id="@+id/rl_set_clear"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:layout_marginTop="6dp"
        android:onClick="click"
        android:background="@drawable/em_btn_bottom_selector"
        android:visibility="visible">
        <TextView
            android:id="@+id/tv_set_clear"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingLeft="20dip"
            android:paddingRight="20dip"
            android:text="@string/set_clear"
            android:textColor="@android:color/black"
            android:textSize="@dimen/size_set"
            android:typeface="serif" />
        <TextView
            android:id="@+id/tv_cache_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:text="256.2m"
            android:textColor="@android:color/black" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:layout_alignParentBottom="true"
            android:background="#E6E6E6" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_set_help"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:layout_marginTop="6dp"
        android:onClick="click"
        android:background="@drawable/em_btn_bottom_selector"
        android:visibility="visible">
